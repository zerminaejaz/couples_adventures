<%= form_for @entry do |f|%>
    <%= f.hidden_field :diary_id, value: @logged_in_user.diaries.first.id%>
    <%= f.label :date %>
    <%= f.date_field :date %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :picture_id %>
    <%= f.collection_select :picture_id, Picture.all,:id, :url  %> <!--update after session lesson  -->

    <%= f.submit %>
<%end%>